{
  "service": {
    "elasticsearch": {
      "type": "elasticsearch"
    },
    "mysql": {
      "type": "mysql"
    },
    "redis": {
      "type": "redis"
    },
    "kafka": {
      "type": "kafka"
    },
    "api": {
      "type": "http_mesh"
    },
    "goodbye": {
      "type": "grpc_mesh",
      "protocols": ["test.Test"]
    },
    "envoy_auth": {
      "type": "grpc_direct",
      "protocols": [
        "envoy.service.auth.v2.Authorization"
      ],
      "port": 12400
    },
    "userauth": {
      "type": "grpc_mesh",
      "protocols": [
        "mesh.auth.UserAuthService"
      ]
    },
    "data_management": {
      "type": "grpc_mesh",
      "protocols": [
        "mesh.data.DataManagementService"
      ]
    }
  },
  "provider": {
    "api": {
      "type": "mesh_node",
      "dns": "api.development.svc.cluster.local"
    },
    "worker-io": {
      "type": "mesh_node",
      "dns": "worker-io.development.svc.cluster.local"
    },
    "kafka_development": {
      "type": "kafka",
      "brokers": ["kafka-kafka-brokers.development.svc.cluster.local:9092"]
    },
    "redis_development": {
      "type": "redis",
      "host": "redis.development.svc.cluster.local",
      "port": 6379,
      "db": 0
    },
    "mysql_development": {
      "type": "mysql",
      "host": "mysql.default.svc",
      "port": 3306,
      "database": "app_development",
      "user": "app",
      "passwordSecretId": "MYSQL_PASSWORD"
    },
    "elasticsearch_development": {
      "type": "elasticsearch",
      "nodes": ["https://es-primary-es-http.default.svc:9200"],
      "user": "app",
      "indexPrefix": "development_",
      "passwordSecretId": "ELASTICSEARCH_PASSWORD"
    }
  },
  "environment": {
    "live": {
      "service": {
        "api": {},
        "goodbye": {},
        "envoy_auth": {},
        "userauth": {},
        "kafka": {},
        "redis": {},
        "mysql": {},
        "elasticsearch": {},
        "data_management": {}
      },
      "provider": {
        "api": {},
        "worker-io": {},
        "kafka_development": {},
        "mysql_development": {},
        "elasticsearch_development": {},
        "redis_development": {}
      },
      "providerMap": {
        "api": "api",
        "envoy_auth": "worker-io",
        "goodbye": "worker-io",
        "data_management": "worker-io",
        "userauth": "worker-io",
        "kafka": "kafka_development",
        "mysql": "mysql_development",
        "redis": "redis_development",
        "elasticsearch": "elasticsearch_development"
      }
    }
  }
}
